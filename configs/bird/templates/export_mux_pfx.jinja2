if ( net = {{prefix}} ) then {
    {%- if spec.get('origin') %}
    bgp_path.prepend({{spec.get('origin')}});
    {%- for asn in spec.get('prepend', []) | reverse %}
    bgp_path.prepend({{asn}});
    {%- endfor %}
    {%- endif %}
    {%- for c in spec.get('peers_ids', []) %}
    bgp_community.add((47065,{{c}}));
    {%- endfor %}
    {%- for c1, c2 in spec.get('communities', []) %}
    bgp_community.add(({{c1}}:{{c2}}));
    {%- endfor %}
    accept;
}
